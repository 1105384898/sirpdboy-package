
config crontab
	option week '*'
	option hour '5'
	option day '*'
	option month '*'
	option enable '0'
	option minute '30'
	option command 'sync && echo 3 > /proc/sys/vm/drop_caches'

config crontab
	option enable '0'
	option month '*'
	option day '*'
	option hour '5'
	option minute '0'
	option command 'sleep 5 && touch /etc/banner && reboot'
	option week '1,3,5'

config crontab
	option enable '0'
	option month '*'
	option day '*'
	option week '*'
	option minute '0'
	option hour '*/4'
	option command '/etc/init.d/network restart'

config crontab
	option enable '0'
	option month '*'
	option day '*'
	option week '1-5'
	option hour '7'
	option minute '50'
	option command 'wifi up'

config crontab
	option enable '0'
	option month '*'
	option day '*'
	option minute '0'
	option week '1-5'
	option hour '18'
	option command 'wifi down'

